package geometria;

public class Circulo extends FiguraGeometrica {
	private int radio;

	public Circulo() {
		// TODO Auto-generated constructor stub
	}

	public Circulo(int radio) {
		super();
		this.radio = radio;
	}

	public int getRadio() {
		return radio;
	}

	public void setRadio(int radio) {
		this.radio = radio;
	}

}
